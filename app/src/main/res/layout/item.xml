<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:scrollbars="horizontal"
    android:layout_marginBottom="20dp"
    tools:context=".view.AddHosRegisterActivity"
    tools:ignore="RtlSymmetry">

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignParentTop="true"
        android:layout_gravity="center_vertical"
        android:background="#66CCFF"
        android:visibility="visible" />

    <LinearLayout
        android:id="@+id/panel_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
            android:id="@+id/text_hos_name"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:text="姓名"
            android:textSize="20dp"
            tools:ignore="HardcodedText,SpUsage" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
            android:id="@+id/hos_r_name"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:lines="1"
            android:textSize="20sp" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/panel_1"
        android:layout_gravity="center_vertical"
        android:background="#66CCFF"
        android:visibility="visible" />

    <LinearLayout
        android:id="@+id/panel_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/panel_1"
        android:orientation="horizontal">

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
            android:id="@+id/text_hos_idcard"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:text="身份证号"
            android:textSize="20dp"
            tools:ignore="HardcodedText,SpUsage,TextViewEdits" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
            android:id="@+id/hos_r_id_card"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:lines="1"
            android:maxLength="18"
            android:textSize="20dp"
            tools:ignore="SpUsage" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/panel_2"
        android:layout_gravity="center_vertical"
        android:background="#66CCFF" />

    <LinearLayout
        android:id="@+id/panel_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/panel_2"
        android:orientation="horizontal">

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
            android:id="@+id/text_hos_medical"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:text="社保号"
            android:textSize="20dp"
            tools:ignore="HardcodedText,SpUsage" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
            android:id="@+id/hos_r_medical"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:lines="1"
            android:maxLength="19"
            android:textSize="20dp"
            tools:ignore="SpUsage" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/panel_3"
        android:layout_gravity="center_vertical"
        android:background="#66CCFF" />

    <LinearLayout
        android:id="@+id/panel_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/panel_3"
        android:orientation="horizontal">

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
            android:id="@+id/text_hos_reg_price"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:text="挂号费"
            android:textSize="20dp"
            tools:ignore="HardcodedText,SpUsage" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
            android:id="@+id/hos_r_reg_price"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:textSize="20dp"
            tools:ignore="SpUsage" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/panel_4"
        android:layout_gravity="center_vertical"
        android:background="#66CCFF" />

    <LinearLayout
        android:id="@+id/panel_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/panel_4"
        android:orientation="horizontal">

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
            android:id="@+id/text_hos_phone"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:text="联系电话"
            android:textSize="20dp"
            tools:ignore="HardcodedText,SpUsage" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
            android:id="@+id/hos_r_phone"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:lines="1"
            android:maxLength="11"
            android:textSize="20dp"
            tools:ignore="SpUsage" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/panel_5"
        android:layout_gravity="center_vertical"
        android:background="#66CCFF" />

    <LinearLayout
        android:id="@+id/panel_6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/panel_5"
        android:orientation="horizontal">

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
