            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
            android:id="@+id/hos_r_self_price"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:lines="1"
            android:maxLength="11"
            android:textSize="20dp"
            tools:ignore="SpUsage" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/panel_6"
        android:layout_gravity="center_vertical"
        android:background="#66CCFF" />

    <LinearLayout
        android:id="@+id/panel_7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/panel_6"
        android:orientation="horizontal">

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
            android:id="@+id/text_hos_sex"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:text="性别"
            android:textSize="20dp"
            tools:ignore="HardcodedText,SpUsage" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
            android:id="@+id/hos_r_sex"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:lines="1"
            android:maxLength="11"
            android:textSize="20dp"
            tools:ignore="SpUsage" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/panel_7"
        android:layout_gravity="center_vertical"
        android:background="#66CCFF" />

    <LinearLayout
        android:id="@+id/panel_8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/panel_7"
        android:orientation="horizontal">

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
            android:id="@+id/text_hos_age"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:text="年龄"
            android:textSize="20dp"
            tools:ignore="HardcodedText,SpUsage" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
            android:id="@+id/hos_r_age"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:lines="1"
            android:maxLength="3"
            android:textSize="20dp"
            tools:ignore="SpUsage" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/panel_8"

        android:layout_gravity="center_vertical"
        android:background="#66CCFF" />

    <LinearLayout
        android:id="@+id/panel_9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/panel_8"
        android:orientation="horizontal">

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
            android:id="@+id/text_hos_work"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:text="职业"
            android:textSize="20dp"
            tools:ignore="HardcodedText,SpUsage" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
            android:id="@+id/hos_r_work"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:lines="1"
            android:textSize="20dp"
            tools:ignore="SpUsage" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/panel_9"
        android:layout_gravity="center_vertical"
        android:background="#66CCFF" />

    <LinearLayout
        android:id="@+id/panel_10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/panel_9"
        android:orientation="horizontal">

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
            android:id="@+id/text_hos_look_doctor"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:text="初复诊"
            android:textSize="20dp"
            tools:ignore="HardcodedText,SpUsage" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
            android:id="@+id/hos_r_look_doctor"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:lines="1"
            android:maxLength="11"
            android:textSize="20dp"
            tools:ignore="SpUsage" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/panel_10"
        android:layout_gravity="center_vertical"
        android:background="#66CCFF" />

    <LinearLayout
        android:id="@+id/panel_11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/panel_10"
        android:orientation="horizontal">

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
            android:id="@+id/text_d_keshi"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:text="所挂科室"
            android:textSize="20dp"
            tools:ignore="HardcodedText,SpUsage" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:background="#66CCFF"
            android:visibility="visible" />

        <TextView
            android:id="@+id/d_keshi"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_gravity="center"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:textSize="20dp"
            tools:ignore="SpUsage" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/panel_11"
        android:layout_gravity="center_vertical"
        android:background="#66CCFF" />

    <LinearLayout
        android:id="@+id/panel_12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
