            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_gravity="center"
            android:layout_marginStart="10dp"
            android:gravity="center|start"
            android:text="是否自费"
            android:textSize="20dp"
            tools:ignore="HardcodedText,SpUsage" />

        <View
